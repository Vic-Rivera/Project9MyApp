<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vic.project9myapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_image"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:launchMode="singleTask">
        <activity
            android:name=".MainMenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectWorkout"
            android:label="SelectWorkout" >
        </activity>
        <activity
            android:name=".EditWorkout"
            android:label="@string/title_activity_edit_workout" >
        </activity>
        <activity
            android:name=".AddWorkouts"
            android:label="@string/title_activity_add_workouts" >
        </activity>
        <activity
            android:name=".AddExercises"
            android:label="@string/title_activity_add_exercises" >
        </activity>
        <activity
            android:name=".WorkoutGo"
            android:label="@string/title_activity_workout_go" >
        </activity>
    </application>

</manifest>
